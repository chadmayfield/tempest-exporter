        "defaults": {
          "unit": "fahrenheit",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 32 },
              { "color": "yellow", "value": 80 },
              { "color": "red", "value": 95 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "textMode": "value",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "tempest_feels_like_temperature_celsius{station_name=~\"$station\"} * 9/5 + 32",
          "legendFormat": "Feels Like"
        }
      ]
    },
    {
      "title": "Dew Point",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "fahrenheit",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 32 },
              { "color": "yellow", "value": 80 },
              { "color": "red", "value": 95 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "textMode": "value",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "tempest_dew_point_celsius{station_name=~\"$station\"} * 9/5 + 32",
          "legendFormat": "Dew Point"
        }
      ]
    },
    {
      "title": "Humidity",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "yellow", "value": null },
              { "color": "green", "value": 30 },
              { "color": "blue", "value": 80 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "textMode": "value",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "tempest_relative_humidity_percent{station_name=~\"$station\"}",
          "legendFormat": "Humidity"
        }
      ]
    },
    {
      "title": "Wind",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "velocitymph",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 25 },
              { "color": "red", "value": 45 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "textMode": "value",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "tempest_wind_speed_meters_per_second{station_name=~\"$station\"}",
          "legendFormat": "Wind"
        }
      ]
    },
    {
      "title": "Pressure",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "pressurembar",
          "decimals": 1,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 980 },
              { "color": "green", "value": 1000 },
              { "color": "yellow", "value": 1040 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "textMode": "value",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "tempest_station_pressure_millibars{station_name=~\"$station\"}",
          "legendFormat": "Pressure"
        }
      ]
    },
    {
      "title": "Temperature",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 4 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Temperature",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 5 },
      "fieldConfig": {
        "defaults": {
          "unit": "fahrenheit",
          "color": { "mode": "palette-classic" }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Air Temp" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Feels Like" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Dew Point" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "blue" } }
            ]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "tempest_air_temperature_celsius{station_name=~\"$station\"} * 9/5 + 32",
          "legendFormat": "Air Temp"
        },
        {
          "expr": "tempest_feels_like_temperature_celsius{station_name=~\"$station\"} * 9/5 + 32",
          "legendFormat": "Feels Like"
        },
        {
          "expr": "tempest_dew_point_celsius{station_name=~\"$station\"} * 9/5 + 32",
          "legendFormat": "Dew Point"
        }
      ]
    },
    {
      "title": "Wind",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Wind Speed",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 16, "x": 0, "y": 14 },
      "fieldConfig": {
        "defaults": {
          "unit": "velocitymph",
          "color": { "mode": "palette-classic" }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Gust" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } },
              { "id": "custom.lineStyle", "value": { "fill": "dash", "dash": [10, 10] } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Lull" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } },
              { "id": "custom.lineStyle", "value": { "fill": "dash", "dash": [10, 10] } }
            ]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "tempest_wind_speed_meters_per_second{station_name=~\"$station\"}",
          "legendFormat": "Average"
        },
        {
          "expr": "tempest_wind_gust_meters_per_second{station_name=~\"$station\"}",
          "legendFormat": "Gust"
        },
        {
          "expr": "tempest_wind_lull_meters_per_second{station_name=~\"$station\"}",
          "legendFormat": "Lull"
        }
      ]
    },
    {
      "title": "Wind Direction",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 14 },
      "fieldConfig": {
        "defaults": {
          "unit": "degree",
          "min": 0,
          "max": 360,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 45 },
              { "color": "yellow", "value": 135 },
              { "color": "red", "value": 225 },
              { "color": "purple", "value": 315 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      },
      "targets": [
        {
          "expr": "tempest_wind_direction_degrees{station_name=~\"$station\"}",
          "legendFormat": "Direction"
        }
      ]
    },
    {
      "title": "Sun & Light",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 22 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Sun & Light",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 23 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Solar Radiation" },
            "properties": [
              { "id": "unit", "value": "watt" },
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "yellow" } },
              { "id": "custom.axisPlacement", "value": "left" }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "UV Index" },
            "properties": [
              { "id": "unit", "value": "none" },
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } },
              { "id": "custom.axisPlacement", "value": "right" }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Illuminance" },
            "properties": [
              { "id": "unit", "value": "lux" },
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "#FADE2A" } },
              { "id": "custom.axisPlacement", "value": "right" },
              { "id": "custom.lineStyle", "value": { "fill": "dash", "dash": [10, 10] } }
            ]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "tempest_solar_radiation_watts{station_name=~\"$station\"}",
          "legendFormat": "Solar Radiation"
        },
        {
          "expr": "tempest_uv_index{station_name=~\"$station\"}",
          "legendFormat": "UV Index"
        },
        {
          "expr": "tempest_illuminance_lux{station_name=~\"$station\"}",
          "legendFormat": "Illuminance"
        }
      ]
    },
    {
      "title": "Precipitation & Lightning",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 31 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Precipitation",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 32 },
      "fieldConfig": {
        "defaults": {
          "unit": "lengthmm",
          "color": { "mode": "fixed", "fixedColor": "blue" },
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 60
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "tempest_precipitation_millimeters{station_name=~\"$station\"}",
          "legendFormat": "Rain"
        }
      ]
    },
    {
      "title": "Lightning",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 32 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Strike Count" },
            "properties": [
              { "id": "unit", "value": "short" },
              { "id": "custom.drawStyle", "value": "bars" },
              { "id": "custom.fillOpacity", "value": 50 },
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "yellow" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Avg Distance" },
            "properties": [
              { "id": "unit", "value": "km" },
              { "id": "custom.axisPlacement", "value": "right" },
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "purple" } }
            ]
          }
        ]
      },
      "options": {
        "tooltip": { "mode": "multi", "sort": "desc" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "targets": [
        {
          "expr": "tempest_lightning_strike_count{station_name=~\"$station\"}",
          "legendFormat": "Strike Count"
        },
        {
          "expr": "tempest_lightning_strike_distance_kilometers{station_name=~\"$station\"}",
          "legendFormat": "Avg Distance"
        }
      ]
    },
    {
      "title": "System Health",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 40 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Connection Status",
      "type": "stat",
      "gridPos": { "h": 6, "w": 4, "x": 0, "y": 41 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": { "text": "Disconnected", "color": "red" },
                "1": { "text": "Connected", "color": "green" }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "graphMode": "none",
        "colorMode": "background",
        "textMode": "value",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "tempest_up",
          "legendFormat": "Connection"
        }
      ]
    },
    {
      "title": "Last Observation",
      "type": "stat",
      "gridPos": { "h": 6, "w": 4, "x": 4, "y": 41 },
      "fieldConfig": {
        "defaults": {
          "unit": "dateTimeFromNow",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "graphMode": "none",
        "colorMode": "background",
        "textMode": "value",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "tempest_last_observation_timestamp_seconds{station_name=~\"$station\"} * 1000",
          "legendFormat": "Last Observation"
        }
      ]
    },
    {
      "title": "Battery",
      "type": "gauge",
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 41 },
      "fieldConfig": {
        "defaults": {
          "unit": "volt",
          "min": 2.0,
          "max": 3.0,
          "decimals": 2,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 2.3 },
              { "color": "green", "value": 2.5 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      },
      "targets": [
        {
          "expr": "tempest_battery_volts{station_name=~\"$station\"}",
          "legendFormat": "Battery"
        }
      ]
    },
    {
      "title": "Reconnects / Errors",
      "type": "stat",
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 41 },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 20 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "graphMode": "none",
        "colorMode": "background",
        "textMode": "value_and_name",
        "reduceOptions": { "calcs": ["lastNotNull"] }
      },
      "targets": [
        {
          "expr": "tempest_websocket_reconnects_total",
          "legendFormat": "Reconnects"
        },
        {
          "expr": "tempest_scrape_errors_total",
          "legendFormat": "Scrape Errors"
        }
      ]
    }
  ],
  "schemaVersion": 39,
  "tags": ["weather", "tempest", "prometheus"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "All", "value": "$__all" },
        "definition": "label_values(tempest_air_temperature_celsius, station_name)",
        "hide": 0,
        "includeAll": true,
        "label": "Station",
        "multi": false,
        "name": "station",
        "options": [],
        "query": { "query": "label_values(tempest_air_temperature_celsius, station_name)", "refId": "StandardVariableQuery" },
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "sort": 0,
        "type": "query"
      }
    ]
  },
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": {},
  "timezone": "",
  "title": "Tempest Weather Station",
  "uid": "tempest-exporter",
  "version": 1
}
